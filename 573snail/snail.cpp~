#include <iostream>
using namespace std;



int main(){

  int H, U, D, F;
  
  while(H){
    cin >> H >> U >> D >> F;

    int day = 0;
    int climbed = 0;
    bool succeeded = 0;
    
    while(climbed <= H){
      climbed += U;
      climbed -= 1;
      U -= 0.3*U;
      day +=1;
      cout << "climbed" << climbed << " +distance: " << U;
    }

    if(climbed >= H){
      succeeded = 1;
    }
    
  
    if(succeeded){
      cout << "success on day" << day;
    }else{
      cout << "failure on day" << day;
    }
  }
  
  return 0;
}

